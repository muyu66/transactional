  },
  "homepage": "https://github.com/muyu66/transactional#readme",
  "devDependencies": {
    "@types/cls-hooked": "^4.3.0",
    "@types/sequelize": "^4.27.44",
    "ava": "^1.3.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "mysql2": "^1.6.5",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "sequelize": "^4.43.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "cls-hooked": "^4.2.2"
